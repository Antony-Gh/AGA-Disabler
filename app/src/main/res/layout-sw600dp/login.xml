<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:id="@+id/linear1"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:gravity="center_horizontal|top"
	android:orientation="vertical"
	android:padding="20dp">

	<LinearLayout
		android:id="@+id/lin1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="horizontal"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		android:padding="10dp">

		<TextView
			android:id="@+id/textview11"
			android:layout_width="wrap_content"
			android:layout_height="40dp"
			android:gravity="center"
			android:text="@string/app_name"
			android:textAppearance="@style/merrifont"
			android:textColor="?attr/colorPrimary"
			android:textSize="30sp"
			android:textStyle="bold" />
	</LinearLayout>

	<LinearLayout
		android:id="@+id/linear35"
		android:layout_width="match_parent"
		android:layout_height="100dp"
		android:layout_marginBottom="8dp"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="horizontal"
		android:paddingTop="4dp"
		android:paddingBottom="4dp"
		app:layout_constraintTop_toBottomOf="@+id/lin1"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent">

		<ImageView
			android:id="@+id/imageview9"
			android:layout_width="70dp"
			android:layout_height="70dp"
			android:layout_marginLeft="6dp"
			android:layout_marginRight="6dp"
			android:layout_weight="1"
			android:contentDescription="@string/instagram"
			android:scaleType="fitCenter"
			android:src="@drawable/instaimg" />

		<ImageView
			android:id="@+id/imageview10"
			android:layout_width="70dp"
			android:layout_height="70dp"
			android:layout_marginLeft="6dp"
			android:layout_marginRight="6dp"
			android:layout_weight="1"
			android:contentDescription="@string/whats_app"
			android:scaleType="fitCenter"
			android:src="@drawable/whatsimg" />

		<ImageView
			android:id="@+id/imageview11"
			android:layout_width="70dp"
			android:layout_height="70dp"
			android:layout_marginLeft="6dp"
			android:layout_marginRight="6dp"
			android:layout_weight="1"
			android:contentDescription="@string/facebook"
			android:scaleType="fitCenter"
			android:src="@drawable/faceimg" />

		<ImageView
			android:id="@+id/imageview12"
			android:layout_width="70dp"
			android:layout_height="70dp"
			android:layout_marginLeft="6dp"
			android:layout_marginRight="6dp"
			android:layout_weight="1"
			android:contentDescription="@string/telegram"
			android:scaleType="fitCenter"
			android:src="@drawable/teleimg" />

		<ImageView
			android:id="@+id/imageview13"
			android:layout_width="70dp"
			android:layout_height="70dp"
			android:layout_marginLeft="6dp"
			android:layout_marginRight="6dp"
			android:layout_weight="1"
			android:contentDescription="@string/youtube"
			android:scaleType="fitCenter"
			android:src="@drawable/youtubeimg" />

		<ImageView
			android:id="@+id/imageview21"
			android:layout_width="70dp"
			android:layout_height="70dp"
			android:layout_marginLeft="6dp"
			android:layout_marginRight="6dp"
			android:layout_weight="1"
			android:contentDescription="@string/twitter"
			android:scaleType="fitCenter"
			android:src="@drawable/twitterimg" />
	</LinearLayout>

	<TextView
		android:id="@+id/textview2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginBottom="15dp"
		android:gravity="center"
		app:layout_constraintTop_toBottomOf="@+id/linear35"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		android:text="@string/welcome_to_my_app"
		android:textColor="?attr/colorPrimary"
		android:textAppearance="@style/merrifont"
		android:textSize="26sp" />

	<LinearLayout
		android:id="@+id/all"
		android:layout_width="match_parent"
		android:layout_height="80dp"
		android:baselineAligned="false"
		android:gravity="center|start"
		android:orientation="horizontal"
		app:layout_constraintTop_toBottomOf="@+id/textview2"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		android:padding="5dp">

		<ImageView
			android:id="@+id/imageview8"
			android:layout_width="50dp"
			android:layout_height="50dp"
			android:contentDescription="@string/license_key"
			android:padding="8dp"
			android:scaleType="fitCenter"
			app:tint="?attr/colorPrimary"
			android:src="@drawable/ic_vpn_key_white" />

		<com.google.android.material.textfield.TextInputLayout
			android:id="@+id/textinputlayout5"
			style="@style/Widget.AGADisabler.TextInputLayout.OutlinedBox"
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:layout_weight="1"
			android:gravity="start|center"
			android:padding="8dp">

			<com.aga.disabler.pro.samsung.ClientEditText
				android:id="@+id/lice"
				style="@style/SamsungCursor"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:gravity="start|center"
				android:hint="@string/your_key"
				android:textAppearance="@style/robotofont"
				android:textColor="?attr/colorPrimary"
				android:textSize="18sp" />
		</com.google.android.material.textfield.TextInputLayout>
	</LinearLayout>

	<LinearLayout
		android:id="@+id/linear61"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="10dp"
		android:layout_marginBottom="5dp"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="horizontal"
		app:layout_constraintTop_toBottomOf="@+id/all"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent">

		<TextView
			android:id="@+id/textview19"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:text="@string/key"
			android:textAppearance="@style/merrifont"
			android:textColor="?attr/colorError"
			android:textSize="18sp" />
	</LinearLayout>

	<LinearLayout
		android:id="@+id/signin"
		android:layout_width="match_parent"
		android:layout_height="80dp"
		android:layout_marginTop="20dp"
		android:layout_marginBottom="20dp"
		android:gravity="center"
		android:orientation="horizontal"
		android:padding="5dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent">

		<ProgressBar
			android:id="@+id/aga_progress_bar"
			android:layout_width="80dp"
			android:layout_height="80dp"
			android:padding="8dp"
			android:scaleType="fitXY" />

		<Button
			android:id="@+id/stnbut"
			android:layout_width="250dp"
			android:layout_height="80dp"
			android:gravity="center"
			android:text="@string/sign_in"
			android:textSize="20sp" />
	</LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
